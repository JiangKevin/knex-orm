{
  "name": "knexpress",
  "version": "0.1.1",
  "description": "Knex-based object-relational mapping for JavaScript.",
  "author": "Kristóf Poduszló <kripod@protonmail.com>",
  "license": "MIT",
  "keywords": [
    "orm",
    "knex"
  ],
  "main": "lib/index.js",
  "files": [
    "lib/"
  ],
  "directories": {
    "lib": "./lib"
  },
  "scripts": {
    "start": "babel-node ./example",
    "build": "babel ./src -d ./lib -s",
    "watch": "npm run build -- -w",
    "prepublish": "npm run build",
    "init-db": "rimraf ./dev.sqlite3 && knex migrate:latest && knex seed:run",
    "docs": "documentation readme ./src/index.js -s \"API Reference\"",
    "test": "babel-node ./node_modules/isparta/bin/isparta cover ./tests"
  },
  "dependencies": {
    "inflection": "^1.8.0"
  },
  "peerDependencies": {
    "knex": ">=0.6.10 <0.11.0"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "codeclimate-test-reporter": "^0.3.1",
    "documentation": "^4.0.0-beta2",
    "eslint": "^2.5.1",
    "eslint-config-airbnb": "^6.2.0",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.2",
    "jsdoc-to-markdown": "^1.3.3",
    "knex": "^0.10.0",
    "rimraf": "^2.5.2",
    "sqlite3": "^3.1.2",
    "tape": "^4.5.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kripod/knexpress.git"
  },
  "bugs": {
    "url": "https://github.com/kripod/knexpress/issues"
  }
}
