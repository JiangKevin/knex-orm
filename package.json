{
  "name": "knexpress",
  "version": "0.1.1",
  "description": "Knex-based object-relational mapping for JavaScript.",
  "author": "Kristóf Poduszló <kripod@protonmail.com>",
  "license": "MIT",
  "keywords": [
    "orm",
    "knex"
  ],
  "main": "lib/index.js",
  "files": [
    "lib/"
  ],
  "directories": {
    "lib": "./lib"
  },
  "scripts": {
    "start": "node --es-staging --use-strict ./example",
    "build": "rimraf ./lib && babel ./src -d ./lib -s",
    "prepublish": "npm run build",
    "init-db": "rimraf ./dev.sqlite3 && knex migrate:latest && knex seed:run",
    "docs": "jsdoc2md \"./src/**/*.js\" -t ./jsdoc2md/README.hbs > ./README.md",
    "test": "rimraf ./coverage && node --es-staging --use-strict ./node_modules/istanbul/lib/cli cover ./tests"
  },
  "dependencies": {
    "lodash.snakecase": "^4.0.0",
    "pluralize": "^1.2.1"
  },
  "peerDependencies": {
    "knex": "<0.11.0"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "codeclimate-test-reporter": "^0.3.1",
    "eslint": "^2.5.1",
    "eslint-config-airbnb": "^6.2.0",
    "istanbul": "^0.4.2",
    "jsdoc": "^3.4.0",
    "jsdoc-to-markdown": "^1.3.3",
    "knex": "^0.10.0",
    "rimraf": "^2.5.2",
    "sqlite3": "^3.1.2",
    "tape": "^4.5.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kripod/knexpress.git"
  },
  "bugs": {
    "url": "https://github.com/kripod/knexpress/issues"
  }
}
