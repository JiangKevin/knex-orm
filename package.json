{
  "name": "knex-orm",
  "version": "1.2.0",
  "description": "Knex-based object-relational mapping for JavaScript.",
  "author": "Kristóf Poduszló <kripod@protonmail.com>",
  "license": "MIT",
  "keywords": [
    "orm",
    "knex"
  ],
  "main": "index.js",
  "files": [
    "lib/",
    "legacy/"
  ],
  "directories": {
    "lib": "./lib"
  },
  "scripts": {
    "start": "babel-node ./example",
    "build": "babel ./lib -d ./legacy -s",
    "watch": "npm run build -- -w",
    "init-db": "npm run babel-knex -- migrate:latest && npm run babel-knex -- seed:run",
    "doc": "documentation build ./lib/index.js -o ./docs -f html",
    "test": "npm run init-db && npm run test:quick && npm run build && node -e \"require('./legacy');\"",
    "test:quick": "npm run babel-isparta -- cover ./tests",
    "babel-isparta": "babel-node ./node_modules/isparta/bin/isparta",
    "babel-knex": "babel-node ./node_modules/knex/lib/bin/cli"
  },
  "dependencies": {
    "ajv": "^4.0.4",
    "babel-runtime": "^6.6.1",
    "inflection": "^1.10.0"
  },
  "peerDependencies": {
    "knex": ">=0.6.10 <0.11.0"
  },
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-eslint": "^6.0.3",
    "babel-plugin-transform-async-to-generator": "^6.7.4",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "codeclimate-test-reporter": "^0.3.1",
    "documentation": "^4.0.0-beta2",
    "eslint": "^2.8.0",
    "eslint-config-airbnb-base": "^1.0.3",
    "eslint-plugin-import": "^1.5.0",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.3",
    "jsdoc-to-markdown": "^1.3.4",
    "knex": "^0.10.0",
    "sqlite3": "^3.1.3",
    "tape": "^4.5.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kripod/knex-orm.git"
  },
  "bugs": {
    "url": "https://github.com/kripod/knex-orm/issues"
  }
}
