{
  "name": "knex-orm",
  "version": "1.2.0",
  "description": "Knex-based object-relational mapping for JavaScript.",
  "author": "Kristóf Poduszló <kripod@protonmail.com>",
  "license": "MIT",
  "keywords": [
    "orm",
    "knex"
  ],
  "main": "lib/index.js",
  "files": [
    "lib/"
  ],
  "directories": {
    "lib": "./lib"
  },
  "scripts": {
    "start": "babel-node ./example",
    "build": "babel ./src -d ./lib -s",
    "watch": "npm run build -- -w",
    "prepublish": "npm run build",
    "init-db": "npm run babel-knex -- migrate:latest && npm run babel-knex -- seed:run",
    "doc": "documentation build ./src/index.js -o ./docs -f html",
    "test": "npm run init-db && npm run test:quick && node -e \"require('./lib');\" && eslint ./src",
    "test:quick": "npm run babel-isparta -- cover ./tests",
    "babel-isparta": "babel-node ./node_modules/isparta/bin/isparta",
    "babel-knex": "babel-node ./node_modules/knex/lib/bin/cli"
  },
  "dependencies": {
    "ajv": "^4.0.5",
    "babel-runtime": "^6.6.1",
    "es6-error": "^2.1.1",
    "inflection": "^1.10.0"
  },
  "peerDependencies": {
    "knex": ">=0.6.10 <0.12.0"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-class-properties": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "codeclimate-test-reporter": "^0.3.1",
    "documentation": "^4.0.0-beta2",
    "eslint": "^2.9.0",
    "eslint-config-airbnb-base": "^2.0.0",
    "eslint-plugin-import": "^1.6.1",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.3",
    "knex": "^0.11.0",
    "sqlite3": "^3.1.3",
    "tape": "^4.5.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kripod/knex-orm.git"
  },
  "bugs": {
    "url": "https://github.com/kripod/knex-orm/issues"
  }
}
